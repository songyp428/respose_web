#editorconfig.org

# top-most EditorConfig file
root=true

[*]

# Set default charset
charset = utf-8

# 4 space indentation
indent_size = 4
indent_style = space
# Unix-style newlines with a newline ending every file

insert_final_newline = true
trim_trailing_whitespace = true

[*.md]

trim_trailing_whitespace = flase
